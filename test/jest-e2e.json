{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/motorola/(.*)": "<rootDir>/../libs/motorola/src/$1",
    "@app/motorola": "<rootDir>/../libs/motorola/src",
    "@app/xiaomi/(.*)": "<rootDir>/../libs/xiaomi/src/$1",
    "@app/xiaomi": "<rootDir>/../libs/xiaomi/src",
    "@app/samsung/(.*)": "<rootDir>/../libs/samsung/src/$1",
    "@app/samsung": "<rootDir>/../libs/samsung/src"
  }
}